<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prinventory_mvvm">

    <application
        android:name=".PrinventoryApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/PrinventoryMVVM">
        <activity android:name=".ui.printer.PrinterActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.toner.TonerActivity"
            android:noHistory="true">
        </activity>

        <activity android:name=".ui.vendor.VendorActivity"
            android:noHistory="true">
        </activity>

    </application>

</manifest>